{
  "name": "joaquimds/eloquoowp",
  "type": "wordpress-muplugin",
  "description": "An extension to OOWP to store post data in Laravel",
  "minimum-stability": "dev",
  "license": "GPL-3.0",
  "version": "0.1",
  "autoload": {
    "psr-4": {
      "Outlandish\\Wordpress\\Eloquoowp\\": "src/"
    }
  },
  "authors": [
    {
      "name": "Joaquim de Souza",
      "email": "joaquim@outlandish.com"
    }
  ],
  "repositories": {
    "wpackagist": {
      "type": "composer",
      "url": "https://wpackagist.org"
    },
    "oowp": {
      "type": "vcs",
      "url": "https://github.com/outlandishideas/oowp.git"
    }
  },
  "require": {
    "illuminate/database": "^5.9",
    "illuminate/events": "^5.9",
    "phpoption/phpoption": "^1.3",
    "vlucas/phpdotenv": "^3.4",
    "composer/installers": "~1.0",
    "outlandish/oowp": "dev-delete-post-hook"
  }
}
